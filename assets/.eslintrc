{
  "extends": [
    "airbnb",
    "plugin:vue/essential"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true,
      "optionalDependencies": false
    }],
    "no-console": "off",
    "no-new": "off",
    "no-param-reassign": "off",
    "max-len": 0
  }
}